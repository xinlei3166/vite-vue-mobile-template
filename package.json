    "@types/qs": "^6.9.7",
    "@types/sortablejs": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@unocss/postcss": "^0.53.4",
    "@unocss/preset-rem-to-px": "^0.53.4",
