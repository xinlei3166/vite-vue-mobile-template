    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.20.0",
    "less": "^4.1.3",
    "less-loader": "^8.1.1",
    "lint-staged": "^10.5.4",
    "mockjs": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "picocolors": "latest",
    "postcss": "^8.4.23",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-pxtorem": "^6.0.0",
