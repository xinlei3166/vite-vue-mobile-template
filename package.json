    "@vueuse/core": "^10.1.2",
    "amfe-flexible": "^2.2.1",
    "animate.css": "^4.1.1",
    "await-to-js": "^3.0.0",
    "axios": "^0.27.2",
    "better-scroll": "^2.5.1",
    "dayjs": "^1.11.7",
    "echarts": "^5.4.3",
    "js-cookie": "^3.0.5",
    "mitt": "^3.0.0",
    "nanoid": "^4.0.2",
    "nprogress": "^0.2.0",
